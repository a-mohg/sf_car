{% extends "admin/base.html.twig" %}

{% block main %}


	{% if car.published %}
		<h2>{{ car.nom }}</h2>
		<h3>{{ car.year | date('d/m/Y') }}</h3>
        <p>{{ car.engine }}</p>
		<p>{{ car.description }}</p>
		<h3>{{ car.groupe.name }}</h3>
		<h4>{{ car.Brand.name }}</h4>
	{% endif %}

{% endblock %}